service fakeperm /system/bin/fakeperm
    class main
    user root

service b2g /system/bin/b2g.sh
    class main
    onrestart restart media

service rilproxy /system/bin/rilproxy
    class main
    socket rilproxy stream 660 root system
    user root
    group radio

on boot
    exec /system/bin/rm -r /data/local/tmp
